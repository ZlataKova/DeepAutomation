    "import numpy as np\n",
    "import random\n",
    "\n",
    "def flip_dN(N:int = 2, times: int = 1) -> list[int]:\n",
    "    return[random.randint(1,N) for _ in range(times)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
